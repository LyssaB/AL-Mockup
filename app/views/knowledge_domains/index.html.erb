<p id="notice"><%= notice %></p>

<h1>Knowledge Domains</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Score img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @knowledge_domains.each do |knowledge_domain| %>
      <tr>
        <td><%= knowledge_domain.name %></td>
        <td><%= knowledge_domain.description %></td>
        <td><%= knowledge_domain.score_img %></td>
        <td><%= link_to 'Show', knowledge_domain %></td>
        <td><%= link_to 'Edit', edit_knowledge_domain_path(knowledge_domain) %></td>
        <td><%= link_to 'Destroy', knowledge_domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Knowledge Domain', new_knowledge_domain_path %>
